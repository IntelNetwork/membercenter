<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.smartwork.dal.mapper.ext.ZGSettApplExtMapper">
  <resultMap id="BaseResultMap" type="org.smartwork.dal.entity.ZGSettAppl" extends="org.smartwork.dal.mapper.SysUserMapper.BaseResultMap">
  </resultMap>
  <sql id="Base_Column_List">
    <include refid="org.smartwork.dal.mapper.SysUserMapper.Base_Column_List" />
  </sql>
  <!--多条件查询用户+分页-->
  <select id="pageTransfer"  resultMap="UserBaseResultMap">
    SELECT
    <include refid="Base_Column_List"/>
    FROM
    fb_zg_sett_appl AS settl
    <where>
      AND settl.review_status=#{reviewStatus,jdbcType=VARCHAR}
      AND settl.settl_status=#{settlStatus,jdbcType=VARCHAR}
      AND settl.failure_count <![CDATA[ <= ]]> settl.retry_count
    </where>
  </select>
</mapper>